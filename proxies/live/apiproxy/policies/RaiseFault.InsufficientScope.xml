<RaiseFault async="false" continueOnError="false" enabled="true" name="RaiseFault.InsufficientScope">
  <FaultResponse>
    <Set>
      <Headers />
      <Payload contentType="text/plain" />
      <StatusCode>401</StatusCode>
      <ReasonPhrase>Unauthorized</ReasonPhrase>
    </Set>
    <AssignVariable>
      <Name>pds.error.code</Name>
      <Value>token</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>pds.error.coding.code</Name>
      <Value>INVALID_SCOPE</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>pds.error.coding.display</Name>
      <Value>Access token has invalid scope for your request</Value>
    </AssignVariable>
    <AssignVariable>
      <Name>pds.error.diagnostics</Name>
      <Template>Your app has insufficient permissions to use this method. Please contact support.</Template>
    </AssignVariable>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
