<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage.AddAccessControlExposeHeader">
  <FaultRules/>
  <Properties/>
  <Set>
      <Headers>
          <Header name="Access-Control-Expose-Headers">x-correlation-id, x-request-id</Header>
      </Headers>
  </Set>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>
